{

  "manifest_version": 2,

  "name": "RefMeNot",
  "version": "1.0",
  "description": "Removes tracking ids from facebook's outbound links.",

  "content_scripts": [
    { 
      "run_at" :"document_end",
      "matches": ["*://*.facebook.com/*"],   
      "js": ["jquery-1.11.3.min.js","cleanup.js"]
    }
  ],

  "icons": {
      "48": "icons/broken-link.png"
    }
}